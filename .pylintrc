[VARIABLES]
init-import=yes

[MESSAGES CONTROL]
disable=
  duplicate-code,
  fixme,
  global-statement,
  import-outside-toplevel,
  invalid-name,
  invalid-unary-operand-type,
  missing-docstring,
  protected-access,
  too-many-ancestors,
  too-many-arguments,
  too-many-branches,
  too-many-instance-attributes,
  too-many-lines,
  too-many-locals,
  too-many-public-methods,
  too-many-return-statements,
  too-many-statements,
  unsubscriptable-object,
  unused-wildcard-import,

[IMPORT]
allow-wildcard-with-all=yes

[FORMAT]
good-names=df,db,x,y,w,ix,jx,ip,io,ws,v,r,tz,gc,p,q,t,f,a,b,fn,th,e,_,pf,m,n,cm,c,fs,lr,k,ax,el,s,ci,le,ms,x1,x2,y1,y2,op,ft,qa,nl,tp,fp,tn,fn
bad-names=i,j

[TYPECHECK]
ignored-modules=dvc
